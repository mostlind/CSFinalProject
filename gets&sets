	//get and set for all variables
	
	public void setAN(int acctNum){
		this.acctNum = acctNum;
		
		try{
		BufferedWriter accountFile = new BufferedWriter(new FileWriter("/CSFinalProject/" + getAN() + ".txt"));
			accountFile.write(String.valueOf(getAN()));
			accountFile.newLine();
			accountFile.write(getLN());
			accountFile.newLine();
			accountFile.write(getFN());
			accountFile.newLine();
			accountFile.write(String.valueOf(getBal()));;
			accountFile.newLine();
			accountFile.write(String.valueOf(getAcc()));
			accountFile.newLine();
			
			accountFile.close();
		}
		
		catch (IOException e){
			e.printStackTrace();
		}
		
	}
	
	public int getAN(){
		return acctNum;
	


