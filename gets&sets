	//get and set for all variables
	
	public void setAN(int acctNum){
		this.acctNum = acctNum;
		accountFile = newPrintWriter(new FileOutputStream(anAccountNumber + ".txt"))
	}
	
	public int getAN(){
		return acctNum;
	}
	
	public void setFN(String firstName){
		this.firstName = firstName;
	}
	
	public String getFN(){
		return firstName;
	}
	
	public void setLN(String lastName){
		this.lastName = lastName;
	}
	
	public String getLN(){
		return lastName;
	}
	
	public void setBal(double balance){
		this.balance = balance;
	}
	
	public double getBal(){
		return balance;
	}
